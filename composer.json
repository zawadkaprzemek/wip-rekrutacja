{
    "name": "dunglas/symfony-docker-skeleton",
    "type": "project",
    "license": "proprietary",
    "description": "Project description",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "symfony/flex": "1.0.x-dev",
        "symfony/framework-bundle": "3.3.x-dev",
        "symfony/yaml": "3.3.x-dev"
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/fabpot/flex" }
    ],
    "conflict": {
        "symfony/symfony": "<3.3",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "",
            "allow-contrib": false
        }
    }
}
